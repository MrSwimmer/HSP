<idea-plugin>
    <id>memeplugin.memeplugin</id>
    <name>Hackathon Starter Pack</name>
    <vendor email="memebattlecompany@gmail.com" url="https://mems.fun/">MemeBattle</vendor>

    <description>It will help you to create skeleton of Android application by Clean Architectre with using Dagger 2, Retrofit 2 and Room</description>

    <extensions defaultExtensionNs="com.intellij">

    </extensions>

    <actions>
        <group id="Start.TopMenu"
               text="_StarterPack"
               description="Hackathon StarterPack">
            <add-to-group group-id="MainMenu" anchor="last"/>
            <action id="Start"
                    class="Start"
                    text="_Start"
                    description="Start Plugin"/>
        </group>
    </actions>
</idea-plugin>